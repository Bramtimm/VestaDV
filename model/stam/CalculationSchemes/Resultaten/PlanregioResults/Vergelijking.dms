																			Scode == 's5a' ? Results_s5a/results/H06_Vraag_LTwarmte : 0.0[GJ_yr_WEQ];
																			
			attribute<GJ_yr_ha>		H07_Warmte_hectare		(Planregio):=	Scode == 's1a' ? Results_s1a/results/H07_Warmte_hectare : 
																			Scode == 's1b' ? Results_s1b/results/H07_Warmte_hectare : 
