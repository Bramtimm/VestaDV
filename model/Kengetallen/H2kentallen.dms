container H2kentallen
{
	//CONSTANTEN
	parameter<mj_kg>		H2energie_onder :=	  120.000 [mj_kg]        ,	Source = "CE Delft parameters H2 pp. 6 Tabel  1",	Descr = "Energieinhoud van waterstof - deze wordt gebruikt";
	parameter<mj_kg>		H2energie_boven :=	  141.800 [mj_kg]        ,	Source = "CE Delft parameters H2 pp. 6 Tabel  1",	Descr = "Energieinhoud van waterstof - deze wordt niet gebruikt";

	//OPEX
	parameter<_yr>			R_oh_ketel      :=	  00.0200 [_yr]          ,	Source = "CE Delft parameters H2 pp.21 Tabel 15",	Descr = "jaarlijkse onderhoudskosten inpandige distributie als aandeel van de initiele investering";
	parameter<_yr>			R_oh_aansl      :=	  00.0375 [_yr]          ,	Source = "CE Delft parameters H2 pp.18 Tabel 13",	Descr = "jaarlijkse onderhoudskosten aansluiting als aandeel van de initiele investering";
	parameter<ratio>		R_lv_admin      :=	  00.2500 [ratio]        ,	Source = "Gelijk warmtenetleveranciers (FO 3.0)",	Descr = "jaarlijkse administratieve lasten voor leverancier als aandeel van het vastrecht";
	
	//EFFICIENCY H2KETEL
	parameter<ratio>		Eff_H2_RV_min   :=	  001.140 [ratio]        ,	Source = "CE Delft parameters H2 pp.21 Tabel 15",	Descr = "minimale efficiency van ruimteverwarming met een waterstofketel";
	parameter<ratio>		Eff_H2_RV_max   :=	  001.140 [ratio]        ,	Source = "CE Delft parameters H2 pp.21 Tabel 15",	Descr = "maximale efficiency van ruimteverwarming met een waterstofketel";
	parameter<ratio>		Eff_H2_TW_min   :=	  000.700 [ratio]        ,	Source = "CE Delft parameters H2 pp.21 Tabel 15",	Descr = "efficiency van ruimteverwarming met een waterstofketel";
	parameter<ratio>		Eff_H2_TW_max   :=	  000.700 [ratio]        ,	Source = "CE Delft parameters H2 pp.21 Tabel 15",	Descr = "efficiency van ruimteverwarming met een waterstofketel";
	parameter<ratio>		PE_cap_min      :=	  00.0727 [ratio]        ,	Source = "CE Delft FO conversietechniek 11-2019",	Descr = "pompenergie";
	parameter<ratio>		PE_cap_max      :=	  00.0727 [ratio]        ,	Source = "CE Delft FO conversietechniek 11-2019",	Descr = "pompenergie";
	
	//EFFICIENCY HYBRIDE WP
	parameter<ratio>		SPF_WP_RV_A_min :=	  004.270 [ratio]        ,	Source = "CE Delft FO conversietechniek 11-2019",	Descr = "minimale efficiency elektrische warmtepomp (hybride) bij schillabel A";
	parameter<ratio>		SPF_WP_RV_A_max :=	  004.270 [ratio]        ,	Source = "CE Delft FO conversietechniek 11-2019",	Descr = "maximale efficiency elektrische warmtepomp (hybride) bij schillabel A";
	parameter<ratio>		SPF_WP_RV_B_min :=	  004.280 [ratio]        ,	Source = "CE Delft FO conversietechniek 11-2019",	Descr = "minimale efficiency elektrische warmtepomp (hybride) bij schillabel B";
	parameter<ratio>		SPF_WP_RV_B_max :=	  004.280 [ratio]        ,	Source = "CE Delft FO conversietechniek 11-2019",	Descr = "maximale efficiency elektrische warmtepomp (hybride) bij schillabel B";
	parameter<ratio>		SPF_WP_RV_C_min :=	  004.290 [ratio]        ,	Source = "CE Delft FO conversietechniek 11-2019",	Descr = "minimale efficiency elektrische warmtepomp (hybride) bij schillabel C";
	parameter<ratio>		SPF_WP_RV_C_max :=	  004.290 [ratio]        ,	Source = "CE Delft FO conversietechniek 11-2019",	Descr = "maximale efficiency elektrische warmtepomp (hybride) bij schillabel C";
	parameter<ratio>		Dekking_WP_A_min:=	  000.580 [ratio]        ,	Source = "CE Delft FO conversietechniek 11-2019",	Descr = "minimale aandeel warmtevraag ingevuld met WP (hybride) bij schillabel A";
	parameter<ratio>		Dekking_WP_A_max:=	  000.580 [ratio]        ,	Source = "CE Delft FO conversietechniek 11-2019",	Descr = "maximale aandeel warmtevraag ingevuld met WP (hybride) bij schillabel A";
	parameter<ratio>		Dekking_WP_B_min:=	  000.535 [ratio]        ,	Source = "CE Delft FO conversietechniek 11-2019",	Descr = "minimale aandeel warmtevraag ingevuld met WP (hybride) bij schillabel B";
	parameter<ratio>		Dekking_WP_B_max:=	  000.535 [ratio]        ,	Source = "CE Delft FO conversietechniek 11-2019",	Descr = "maximale aandeel warmtevraag ingevuld met WP (hybride) bij schillabel B";
	parameter<ratio>		Dekking_WP_C_min:=	  000.490 [ratio]        ,	Source = "CE Delft FO conversietechniek 11-2019",	Descr = "minimale aandeel warmtevraag ingevuld met WP (hybride) bij schillabel C";
	parameter<ratio>		Dekking_WP_C_max:=	  000.490 [ratio]        ,	Source = "CE Delft FO conversietechniek 11-2019",	Descr = "maximale aandeel warmtevraag ingevuld met WP (hybride) bij schillabel C";
	
	//VERGOEDINGEN
	parameter<eur_aansl>	Ki_pm_min       :=	  000.000 [eur_aansl]    ,	Source = "20-11-2019 Werkwaarde 24-1-2020 (nul)",	Descr = "minimale vergoeding voor projectmanagement";
	parameter<eur_aansl>	Ki_pm_max       :=	  000.000 [eur_aansl]    ,	Source = "20-11-2019 Werkwaarde 24-1-2020 (nul)",	Descr = "maximale vergoeding voor projectmanagement";
	parameter<eur_aansl>	Ki_ov_min       :=	  000.000 [eur_aansl]    ,	Source = "20-11-2019 Werkwaarde 24-1-2020 (nul)",	Descr = "minimale vergoeding voor ongerief bewoners";
	parameter<eur_aansl>	Ki_ov_max       :=	  000.000 [eur_aansl]    ,	Source = "20-11-2019 Werkwaarde 24-1-2020 (nul)",	Descr = "maximale vergoeding voor ongerief bewoners";
	
	//CAPEX
	parameter<eur_aansl>	K_aansl_w_min   :=	  200.000 [eur_aansl]    ,	Source = "CE Delft parameters H2 pp.18 Tabel 13",	Descr = "maximale kosten voor het vervangen van een regulieren gasmeter en aansluiting voor een meter die geschikt is voor waterstof bij woningen";
	parameter<eur_aansl>	K_aansl_w_max   :=	  200.000 [eur_aansl]    ,	Source = "CE Delft parameters H2 pp.18 Tabel 13",	Descr = "minimale kosten voor het vervangen van een regulieren gasmeter en aansluiting voor een meter die geschikt is voor waterstof bij woningen";
	parameter<eur_aansl>	K_ketel_w_min   :=	 1861.410 [eur_aansl]    ,	Source = "CE Delft parameters H2 pp.18 Tabel 13",	Descr = "maximale kosten voor het vervangen van een regulieren gasbrander voor een brander die geschikt is voor waterstof bij woningen";
	parameter<eur_aansl>	K_ketel_w_max   :=	 3159.500 [eur_aansl]    ,	Source = "CE Delft parameters H2 pp.18 Tabel 13",	Descr = "minimale kosten voor het vervangen van een regulieren gasbrander voor een brander die geschikt is voor waterstof bij woningen";
	parameter<eur>			K_section_min   :=	 2000.000 [eur]          ,	Source = "CE Delft parameters H2 pp.18 Tabel 13",	Descr = "maximale kosten voor het afsectioneren van het netwerk om het klaar te maken voor waterstof ";
	parameter<eur>			K_section_max   :=	 2000.000 [eur]          ,	Source = "CE Delft parameters H2 pp.18 Tabel 13",	Descr = "minimale kosten voor het afsectioneren van het netwerk om het klaar te maken voor waterstof ";
	parameter<eur_m2>		K_aansl_u_min   :=	 1.538462 [eur_m2]       ,	Source = "CE Delft parameters H2 pp.18 Tabel 13",	Descr = "maximale kosten voor het vervangen van een regulieren gasmeter en aansluiting voor een meter die geschikt is voor waterstof bij utiliteit";
	parameter<eur_m2>		K_aansl_u_max   :=	 1.538462 [eur_m2]       ,	Source = "CE Delft parameters H2 pp.18 Tabel 13",	Descr = "minimale kosten voor het vervangen van een regulieren gasmeter en aansluiting voor een meter die geschikt is voor waterstof bij utiliteit";
	parameter<eur_kw>		K_ketel_u_min   :=	 0095.590 [eur_kw]       ,	Source = "CE Delft parameters H2 pp.20 Tabel 14",	Descr = "minimale kosten voor het vervangen van een regulieren gasbrander voor een brander die geschikt is voor waterstof bij utiliteit";
	parameter<eur_kw>		K_ketel_u_max   :=	 0095.590 [eur_kw]       ,	Source = "CE Delft parameters H2 pp.20 Tabel 14",	Descr = "minimale kosten voor het vervangen van een regulieren gasbrander voor een brander die geschikt is voor waterstof bij utiliteit";
	
	template H2template
	{
		//Case parameters
		parameter<ratio> H2Curve;
		parameter<ratio> OMCurve;
		parameter<ratio> KostenMin;
		parameter<ratio> KostenMax;
		parameter<ratio> EffMax;
		//End case parameters	
		parameter<ratio> EffMin := 1.0 - EffMax;
		
		//KOSTEN
		parameter<eur_aansl>	K_ketel_w        :=		H2Curve * (KostenMin * K_ketel_w_min + KostenMax * K_ketel_w_max);
		parameter<eur_kw>		K_ketel_u        :=		H2Curve * (KostenMin * K_ketel_u_min + KostenMax * K_ketel_u_max);
		parameter<eur_aansl>	K_aansl_w        :=		H2Curve * (KostenMin * K_aansl_w_min + KostenMax * K_aansl_w_max);
		parameter<eur_m2>		K_aansl_u        :=		H2Curve * (KostenMin * K_aansl_u_min + KostenMax * K_aansl_u_max);
		parameter<eur>			K_section        :=		H2Curve * (KostenMin * K_section_min + KostenMax * K_section_max);
		parameter<eur_aansl>	K_ov             :=		OMCurve * (KostenMin * Ki_ov_min     + KostenMax * Ki_ov_max);
		parameter<eur_aansl>	K_pm             :=		OMCurve * (KostenMin * Ki_pm_min     + KostenMax * Ki_pm_max);
		
		//PERFORMANCE
		parameter<ratio>		PompEnergie_cap  :=		EffMin * PE_cap_min       + EffMax * PE_cap_max;
		parameter<ratio>		Eff_H2_RV        :=		EffMin * Eff_H2_RV_min    + EffMax * Eff_H2_RV_max;
		parameter<ratio>		Eff_H2_TW        :=		EffMin * Eff_H2_TW_min    + EffMax * Eff_H2_TW_max;
		parameter<ratio>		SPF_WP_RV_A      :=		EffMin * SPF_WP_RV_A_min  + EffMax * SPF_WP_RV_A_max;
		parameter<ratio>		SPF_WP_RV_B      :=		EffMin * SPF_WP_RV_B_min  + EffMax * SPF_WP_RV_B_max;
		parameter<ratio>		SPF_WP_RV_C      :=		EffMin * SPF_WP_RV_C_min  + EffMax * SPF_WP_RV_C_max;
		parameter<ratio>		Dekking_WP_A     :=		EffMin * Dekking_WP_A_min + EffMax * Dekking_WP_A_max;
		parameter<ratio>		Dekking_WP_B     :=		EffMin * Dekking_WP_B_min + EffMax * Dekking_WP_B_max;
		parameter<ratio>		Dekking_WP_C     :=		EffMin * Dekking_WP_C_min + EffMax * Dekking_WP_C_max;
	}
	
	container Emissies
	{
		parameter<kg_GJ> CO2_GJb := 0[kg_GJ];
		parameter<g_GJ>  NOx_GJb := 0[g_GJ];
		parameter<g_GJ>  SO2_GJb := 0[g_GJ];
		parameter<g_GJ>  VOS_GJb := 0[g_GJ];
		parameter<g_GJ>  TS_GJb  := 0[g_GJ];
	}
}
