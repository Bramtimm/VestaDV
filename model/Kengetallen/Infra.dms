container Infra: using = "units", Source = "MEMO Benno Schippers, 08-05-2016" // nu in euro2018 & TODO: FO 4.0
{
	
	// constanten
	parameter<m2_Aansl>		gem_Aansl_area       :=		 100.00 [m2_Aansl]     ,	Descr = "Gemiddelde oppervlakte van een aangesloten object";
	parameter<Ratio>		AF                   :=		   0.75 [Ratio]        ,	Descr = "aansluitbijdragefactor voor verhouding aansluitbijdrage utiliteit ten opzichte van woningbouw";
	parameter<Ratio>		Winstmarge_g         :=		    0.0 [Ratio]        ,	Descr = "winstmarge van de netbeheerder op het gasnet";
	parameter<Ratio>		Winstmarge_e         :=		    0.0 [Ratio]        ,	Descr = "winstmarge van de netbeheerder op het elektriciteitsnet";
	parameter<m>			L_g_HD_MD            :=		  68332528[m]          ,	Descr = "lengte middendruk en hogedruk gasnetwerk";
	parameter<m>			L_e_HS_MS            :=		   5515556[m]          ,	Descr = "lengte middenspanning en hooogspanning electiciteitnetwerk";
	
	// kostencomponenten aansluiting
	parameter<Eur_Aansl>	AansluitTarief_bw_g  :=		   0.00 [Eur_Aansl]    ,	Descr = "aansluittarief bestaande woningen gas";
	parameter<Eur_Aansl>	AansluitTarief_bw_e  :=		   0.00 [Eur_Aansl]    ,	Descr = "aansluittarief bestaande woningen electriciteit";
	parameter<Eur_Aansl>	AansluitTarief_nw_g  :=		 893.08 [Eur_Aansl]    ,	Descr = "aansluittarief nieuwe woningen gas";
	parameter<Eur_Aansl>	AansluitTarief_nw_e  :=		 623.47 [Eur_Aansl]    ,	Descr = "aansluittarief nieuwe woningen electriciteit";
	parameter<Eur_m2>		AansluitTarief_bu_g  :=		   0.00 [Eur_m2]       ,	Descr = "aansluittarief bestaande utiliteit gas";
	parameter<Eur_m2>		AansluitTarief_bu_e  :=		   0.00 [Eur_m2]       ,	Descr = "aansluittarief bestaande utiliteit electiciteit";
	parameter<Eur_yr_Aansl>	Vastrecht_g          :=		  20.22 [Eur_yr_Aansl] ,	Descr = "vastrecht gas, in brondocument: VR";
	parameter<Eur_yr_Aansl>	Vastrecht_e          :=		  20.22 [Eur_yr_Aansl] ,	Descr = "vastrecht electriciteit, in brondocument: VR";
	parameter<Eur_yr_Aansl>	PA_g                 :=		  23.59 [Eur_yr_Aansl] ,	Descr = "periodieke aansluitvergoeding gas";
	parameter<Eur_yr_Aansl>	PA_e                 :=		  20.22 [Eur_yr_Aansl] ,	Descr = "periodieke aansluitvergoeding electriciteit";
	parameter<Eur_yr_Aansl>	meterhuur_g          :=		  22.47 [Eur_yr_Aansl] ,	Descr = "meterhuur gas, in brondocument: MH";
	parameter<Eur_yr_Aansl>	meterhuur_e          :=		  31.45 [Eur_yr_Aansl] ,	Descr = "meterhuur electriciteit, in brondocument: MH";
	parameter<Eur_yr_Aansl>	capaciteitstarief_g  :=		  73.02 [Eur_yr_Aansl] ,	Descr = "capaciteitstarief gas, in brondocument: CT";
	parameter<Eur_yr_Aansl>	capaciteitstarief_e  :=		 148.28 [Eur_yr_Aansl] ,	Descr = "capaciteitstarief electiciteit, in brondocument: CT";
	parameter<Eur_yr_Aansl>	administatiekosten_g :=		  49.43 [Eur_yr_Aansl] ,	Descr = "administatiekosten leverancier gas";
	parameter<Eur_yr_Aansl>	administatiekosten_e :=		  49.43 [Eur_yr_Aansl] ,	Descr = "administatiekosten leverancier electiciteit";
	
	// kostencomponenten netwerk
	parameter<MEur_yr>		Kj_tr_g              :=		  337.01    [MEur_yr]  ,	Descr = "jaarlijkse kapitaallasten transportnet gas, in brondocument: JKtransport: 150mln";
	parameter<MEur_yr>		Kj_tr_e              :=		  168.51    [MEur_yr]  ,	Descr = "jaarlijkse kapitaallasten transportnet electiciteit, in brondocument: JKtransport";
	parameter<MEur_yr>		Kj_di_g              :=		  617.85    [MEur_yr]  ,	Descr = "jaarlijkse kapitaallasten distributienet gas, in brondocument: JKdistributie: 350mln";
	parameter<MEur_yr>		Kj_di_e              :=		  617.85    [MEur_yr]  ,	Descr = "jaarlijkse kapitaallasten distributienet electiciteit, in brondocument: JKdistributie: 100mln";
	parameter<MEur_yr>		Ko_tr_g              :=		  730.19    [MEur_yr]  ,	Descr = "jaarlijkse onderhoudskosten transportnet gas, in brondocument: JOtransport: 60mln";
	parameter<MEur_yr>		Ko_tr_e              :=		  449.35    [MEur_yr]  ,	Descr = "jaarlijkse onderhoudskosten transportnet electiciteit, in brondocument: JOtransport: 100mln";
	parameter<MEur_yr>		Ko_di_g              :=		 1291.869   [MEur_yr]  ,	Descr = "jaarlijkse onderhoudskosten distributienet gas, in brondocument: JOdistributie: 100mln";
	parameter<MEur_yr>		Ko_di_e              :=		 3201.589   [MEur_yr]  ,	Descr = "jaarlijkse onderhoudskosten distributienet electiciteit, in brondocument: JOdistributie: 250mln";
	parameter<MEur_yr>		O_net_g              :=		 4566.17036 [MEur_yr]  ,	Descr = "onderhoudskosten gasnet jaarlijks";
	parameter<MEur_yr>		O_net_e              :=		 3896.73301 [MEur_yr]  ,	Descr = "onderhoudskosten elektriciteitsnet jaarlijks";

	// afgeleiden
	parameter<Eur_yr_Aansl>	Jaarlijks_w_g        :=		Vastrecht_g + PA_g + meterhuur_g + capaciteitstarief_g + administatiekosten_g;
	parameter<Eur_yr_Aansl>	Jaarlijks_w_e        :=		Vastrecht_e + PA_e + meterhuur_e + capaciteitstarief_e + administatiekosten_e;
	parameter<Eur_m2>		AansluitTarief_nu_g  :=		AF * AansluitTarief_nw_g / gem_Aansl_area;
	parameter<Eur_m2>		AansluitTarief_nu_e  :=		AF * AansluitTarief_nw_e / gem_Aansl_area;
	parameter<Eur_yr_m2>	Jaarlijks_u_g        :=		Jaarlijks_w_g / gem_Aansl_area;
	parameter<Eur_yr_m2>	Jaarlijks_u_e        :=		Jaarlijks_w_e / gem_Aansl_area;
	parameter<MEur_yr>		Kj_g                 :=		Kj_tr_g + Kj_di_g      ,	Descr = "jaarlijkse kapitaallasten gas";
	parameter<MEur_yr>		Ko_g                 :=		Ko_tr_g + Ko_di_g      ,	Descr = "jaarlijkse onderhoudskosten gas";
	parameter<MEur_yr>		Kj_e                 :=		Kj_tr_e + Kj_di_e      ,	Descr = "jaarlijkse kapitaallasten electiciteit";
	parameter<MEur_yr>		Ko_e                 :=		Ko_tr_e + Ko_di_e      ,	Descr = "jaarlijkse onderhoudskosten electiciteit";
	parameter<MEur_yr>		K_g                  :=		Kj_g + Ko_g            ,	Descr = "jaarlijkse kosten gas";
	parameter<MEur_yr>		K_e                  :=		Kj_e + Ko_e            ,	Descr = "jaarlijkse kosten electiciteit";

	parameter<m>			L_g_LD               :=		sum(RuimtelijkeData/infra/per_buurt/input_lengte_LD_net),	Descr = "lengte lagedruk gasnetwerk";
	parameter<m>			L_g                  :=		 L_g_LD + L_g_HD_MD    ,	Descr = "totale lengte gasnetwerk";	
	parameter<m>			L_e_LS               :=		sum(RuimtelijkeData/infra/per_buurt/input_lengte_LS_net),	Descr = "lengte laagspanning electiciteitnetwerk";
	parameter<m>			L_e                  :=		 L_e_LS + L_e_HS_MS    ,	Descr = "totale lengte electiciteitnetwerk";

	// schalingsfactoren
	parameter<float64>		SF_g                 :=		(1.0 - Winstmarge_g) * (O_net_g * 1000000 [eur / meur]) / K_g[Eur_yr],	Descr = "schalingsfactor gas";
	parameter<float64>		SF_e                 :=		(1.0 - Winstmarge_e) * (O_net_g * 1000000 [eur / meur]) / K_e[Eur_yr],	Descr = "schalingsfactor electiciteit";
	parameter<Eur_yr_m>		Kj_g_m               :=		SF_g *Kj_g[Eur_yr] / L_g;
	parameter<Eur_yr_m>		Kj_e_m               :=		SF_e *Kj_e[Eur_yr] / L_e;
	parameter<Eur_yr_m>		Ko_g_m               :=		SF_g *Ko_g[Eur_yr] / L_g;
	parameter<Eur_yr_m>		Ko_e_m               :=		SF_e *Ko_e[Eur_yr] / L_e;
}
