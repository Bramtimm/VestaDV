container LeidraadResultaten: using = "Model"
{
	container StartJaar      := CalculationSchemes/LeidraadResultaten('StartJaar');
	container Ref2030        := CalculationSchemes/LeidraadResultaten(Classifications/RekenStap/Name[uint8(1)])
	{	
		container WarmteBestaand
		{
			attribute<Ratio>  R_StadV         (PlanRegio) := Invoer/RuimtelijkeData/StudieGebied/buurt/R_StadV;
			
			attribute<GJ_yr>  Vj_Bijstook     (PlanRegio) := R_StadV * StateNaAllocatie/KostenBaten/KostenD/Bijstook/Vj_Bijstook;
			attribute<GJ_yr>  Vj_wd_elek      (PlanRegio) := R_StadV * StateNaAllocatie/KostenBaten/KostenD/SecundairNet/V_wd_elek;
			attribute<GJ_yr>  Vj_ow_warmte    (PlanRegio) := R_StadV * StateNaAllocatie/KostenBaten/KostenD/Vj_warmte;
			attribute<Eur_yr> Kj_ow_productie (PlanRegio) := R_StadV * GebiedsAllocatie/RestWarmte/Iterations/I0/Assignment/Km_WarmteFactor * StateNaAllocatie/KostenBaten/Vj_WarmtePrimairD;
			attribute<Eur_yr> Kj_wd_gas       (PlanRegio) := R_StadV * (1d - GebiedsAllocatie/RestWarmte/results/RestWarmte/P_vol) * StateNaAllocatie/KostenBaten/KostenD/Bijstook/Km_wd_gas;
			attribute<Eur_yr> Kj_wd_elek      (PlanRegio) := R_StadV * StateNaAllocatie/KostenBaten/KostenD/SecundairNet/Km_wd_elek;
			attribute<Eur_yr> Kj_lv_admin     (PlanRegio) := R_StadV * StateNaAllocatie/KostenBaten/Opbrengsten/JaarlijksD/Oj_vastrecht * Tussenresultaten/Leercurves/OenM/Curve * Kengetallen/Onderhoud/R_lv_Admin * 1[yr];
			attribute<Eur_yr> Kj_id_oh        (PlanRegio) := R_StadV * StartJaar/StateNaAllocatie/HerKostenBaten/KostenD/Kj_id_oh;
			attribute<Eur_yr> Kj_wd_oh        (PlanRegio) := R_StadV * StartJaar/StateNaAllocatie/HerKostenBaten/KostenD/Kj_wd_oh;
			attribute<Eur_yr> Kj_wd_adm       (PlanRegio) := R_StadV * StartJaar/StateNaAllocatie/HerKostenBaten/KostenD/Kj_wd_adm;
			attribute<Eur_yr> Kj_pt_oh        (PlanRegio) := R_StadV * Startjaar/GebiedsAllocatie/RestWarmte/lastiter/Assignment/Kj_pt_oh;
			attribute<Eur_yr> Kj_pt_adm       (PlanRegio) := R_StadV * Startjaar/GebiedsAllocatie/RestWarmte/lastiter/Assignment/Kj_pt_adm;
			attribute<Eur_yr> Kj_ow_oh        (PlanRegio) := R_StadV * Startjaar/GebiedsAllocatie/RestWarmte/lastiter/Assignment/Kj_ow_oh;
			attribute<Eur_yr> Kj_ow_adm       (PlanRegio) := R_StadV * Startjaar/GebiedsAllocatie/RestWarmte/lastiter/Assignment/Kj_ow_adm;
			attribute<Eur_yr> Kj_gnet_oh      (PlanRegio) := R_StadV * Netwerken/Aardgas/Kj_oh;
			attribute<Eur_yr> Kj_enet_oh      (PlanRegio) := R_StadV * Netwerken/Elektriciteit/Kj_oh;
			attribute<nrAansl>nrAansl_w_MT    (PlanRegio) := R_StadV * StateNaAllocatie/KostenBaten/PlanRegioKaarten/BestaandeWoning/nrAansl_totaal;
			attribute<nrAansl>nrAansl_u_MT    (PlanRegio) := R_StadV * StateNaAllocatie/KostenBaten/PlanRegioKaarten/BestaandeUtil/nrAansl_totaal;	
			}
	}
	container Zichtjaar := CalculationSchemes/LeidraadResultaten(last(Classifications/RekenStap/Name));
}