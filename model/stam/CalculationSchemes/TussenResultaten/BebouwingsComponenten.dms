container BebouwingsComponenten :=
	for_each_ne(Classifications/BebouwingsComponent/name,
		'CalculationSchemes/BCdata('
			'Bebouwing/'+ Classifications/BebouwingsComponent/name +','
			+ quote(Classifications/BebouwingsComponent/name)+','
			+ string(Classifications/BebouwingsComponent/IsNieuwbouw)+','
			+ string(HeeftResterendFactoren)+','
			+(HeeftResterendFactoren ? 'Bebouwing/'+ Classifications/BebouwingsComponent/name +'ResterendFactors' : 'Bebouwing')+','
			'invoer/Kengetallen/Bebouwing/'+ Classifications/BebouwingsComponent/name +'/results,'
			'Classifications/SchilSprong/'+Classifications/BebouwingsComponent/SchilSprong_sel+','
			'SpecifiekeInstellingen/RuimtelijkeVraag/Lokaal/'+ Classifications/BebouwingsComponent/name +','
			'VraagKentallen/'+ Classifications/BebouwingsComponent/name +','
			'PrevState/Bebouwing/'+ Classifications/BebouwingsComponent/name +','
			'Prijzen/AardGas/'+(Classifications/BebouwingsComponent/IsGlastuinbouw ? 'Glastuinbouw' : 'Staffel')+','
			+ quote(Classifications/BebouwingsComponent/NcwRefBase)+','
			+'1.0 '+(Classifications/BebouwingsComponent/DraagtBTW ? '+ SpecifiekeInstellingen/VAT    / 100[Percent]' : '')+','
			+'1.0 '+(Classifications/BebouwingsComponent/DraagtBTW ? '+ SpecifiekeInstellingen/VAT_gv / 100[Percent]' : '')
		+')'
	)
{
	container BestaandeWoning // specific AansluitTarief (per nrAansluitingen)
	{
		unit<uint32> BebouwingsObject;
		container opbrengsten: Source = "FO v7a F 6 en 12"
		{
			attribute<float64> AansluitTarief (BebouwingsObject) := 
				  (VraagKentallen/Opbrengsten/AansluitTarief_min[BebouwingsType] * Schuiven/OpbrMin
				+  VraagKentallen/Opbrengsten/AansluitTarief_max[BebouwingsType] * Schuiven/OpbrMax);

			attribute<Eur>	AansluitBijdrageG  (BebouwingsObject) :=	RuimtelijkeVraagComponent/nrAansluitingen * Kengetallen/Infra/AansluitTarief_bw_g, Descr = "Aansluitbijdrage voor nieuwe aansluiting op het gasnet";
			attribute<Eur>	AansluitBijdrageE  (BebouwingsObject) :=	RuimtelijkeVraagComponent/nrAansluitingen * Kengetallen/Infra/AansluitTarief_bw_e, Descr = "Aansluitbijdrage voor nieuwe aansluiting op het elektriciteitsnetnet";
			attribute<Eur>	AansluitBijdrageD  (BebouwingsObject) :=	AansluitTarief  * WarmteVraag/deelnemers/nrAansluitingen,                          Descr = "Aansluitbijdrage voor nieuwe aansluiting op warmtenetten, gecorrigeerd voor deelname";
			attribute<Eur>	AansluitBijdrageT  (BebouwingsObject) :=	AansluitTarief  * RuimtelijkevraagComponent/nrAansluitingen,                       Descr = "Aansluitbijdrage voor nieuwe aansluiting op warmtenetten, niet gecorrigeerd voor deelname";

			parameter<Eur_yr_Aansl>	VastRechtMT :=
				( VraagKentallen/Opbrengsten/VastRechtMT_Min * Schuiven/OpbrMin 
				+ VraagKentallen/Opbrengsten/VastRechtMT_Max * Schuiven/OpbrMax);
			parameter<Eur_yr_Aansl>	VastRechtLT :=
				( VraagKentallen/Opbrengsten/VastRechtLT_Min * Schuiven/OpbrMin 
				+ VraagKentallen/Opbrengsten/VastRechtLT_Max * Schuiven/OpbrMax);
			parameter<Eur_yr_Aansl>	VastRechtK  :=
				( VraagKentallen/Opbrengsten/VastRechtK_Min  * Schuiven/OpbrMin 
				+ VraagKentallen/Opbrengsten/VastRechtK_Max  * Schuiven/OpbrMax);

			attribute<Eur_yr>	VastRechtBijdrageG (BebouwingsObject) :=	RuimtelijkeVraagComponent/nrAansluitingen * Kengetallen/Infra/JaarlijksVast_g, Descr = "VastRechtBijdrage voor aansluiting op het gasnet";
			attribute<Eur_yr>	VastRechtBijdrageE (BebouwingsObject) :=	RuimtelijkeVraagComponent/nrAansluitingen * Kengetallen/Infra/JaarlijksVast_e, Descr = "VastRechtBijdrage voor aansluiting op het elektriciteitsnetnet";
			attribute<Eur_yr>	VastRechtMT_D      (BebouwingsObject) :=	WarmteVraag/deelnemers/nrAansluitingen    * VastRechtMT,                       Descr = "VastRechtBijdrage voor aansluiting op warmtenetten, gecorrigeerd voor deelname";
			attribute<Eur_yr>	VastRechtMT_T      (BebouwingsObject) :=	RuimtelijkeVraagComponent/nrAansluitingen * VastRechtMT,                       Descr = "VastRechtBijdrage voor aansluiting op warmtenetten, niet gecorrigeerd voor deelname";
			attribute<Eur_yr>	VastRechtLT_D      (BebouwingsObject) :=	WarmteVraag/deelnemers/nrAansluitingen    * VastRechtLT,                       Descr = "VastRechtBijdrage voor aansluiting op warmtenetten, gecorrigeerd voor deelname";
			attribute<Eur_yr>	VastRechtLT_T      (BebouwingsObject) :=	RuimtelijkeVraagComponent/nrAansluitingen * VastRechtLT,                       Descr = "VastRechtBijdrage voor aansluiting op warmtenetten, niet gecorrigeerd voor deelname";
			attribute<Eur_yr>	VastRechtK_D       (BebouwingsObject) :=	WarmteVraag/deelnemers/nrAansluitingen    * VastRechtK,                        Descr = "VastRechtBijdrage voor aansluiting op warmtenetten, gecorrigeerd voor deelname";
			attribute<Eur_yr>	VastRechtK_T       (BebouwingsObject) :=	RuimtelijkeVraagComponent/nrAansluitingen * VastRechtK,                        Descr = "VastRechtBijdrage voor aansluiting op warmtenetten, niet gecorrigeerd voor deelname";
			
			attribute<Eur_yr>	HeffingskortingE   (BebouwingsObject) :=	SpecifiekeInstellingen/Beleid/Heffingskorting * RuimtelijkeVraagComponent/nrAansluitingen;
		}
	}
