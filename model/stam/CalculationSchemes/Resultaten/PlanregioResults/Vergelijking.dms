			attribute<GJ_yr_WEQ>	H05_Vraag_MTwarmte		(Planregio):=	Scode == 's1a' ? Results_s1a/results/H05_Vraag_MTwarmte : 
																			Scode == 's1b' ? Results_s1b/results/H05_Vraag_MTwarmte : 
