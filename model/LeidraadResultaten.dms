container LeidraadResultaten: using = "Model"
{
	container StartJaar      := CalculationSchemes/LeidraadResultaten('StartJaar')
	{
		container WarmteBestaand
		{
			attribute<Ratio>  R_StadV         (PlanRegio) := Invoer/RuimtelijkeData/StudieGebied/buurt/R_StadV;
			
			attribute<GJ_yr>  V_Bijstook      (PlanRegio) := R_StadV * StateNaAllocatie/KostenBaten/KostenD/Bijstook/V_Bijstook;
			attribute<GJ_yr>  V_wd_elek       (PlanRegio) := R_StadV * StateNaAllocatie/KostenBaten/KostenD/SecundairNet/V_wd_elek;
			attribute<GJ_yr>  V_ow_warmte     (PlanRegio) := R_StadV * StateNaAllocatie/KostenBaten/KostenD/V_warmte;
			attribute<Eur_yr> Kj_ow_productie (PlanRegio) := R_StadV * GebiedsAllocatie/RestWarmte/Iterations/I0/Assignment/Km_WarmteFactor * StateNaAllocatie/KostenBaten/V_WarmtePrimairD;
			attribute<Eur_yr> Kj_wd_gas       (PlanRegio) := R_StadV * (1d - GebiedsAllocatie/RestWarmte/results/RestWarmte/P_vol) * StateNaAllocatie/KostenBaten/KostenD/Bijstook/Km_wd_gas;
			attribute<Eur_yr> Kj_wd_elek      (PlanRegio) := R_StadV * StateNaAllocatie/KostenBaten/KostenD/SecundairNet/Km_wd_elek;
			attribute<Eur_yr> Kj_lv_admin     (PlanRegio) := R_StadV * StateNaAllocatie/KostenBaten/Opbrengsten/JaarlijksD/Oj_vastrecht * Tussenresultaten/Leercurves/OenM/Curve * Kengetallen/Onderhoud/R_lv_Admin * 1[yr];
			
			//TODO FvdM: in de volgende 3 attributen echtHerKostenBaten nodig?
			attribute<Eur_yr> Kj_id_oh        (PlanRegio) := R_StadV * StartJaar/StateNaAllocatie/HerKostenBaten/KostenD/Kj_id_oh;
			attribute<Eur_yr> Kj_wd_oh        (PlanRegio) := R_StadV * StartJaar/StateNaAllocatie/HerKostenBaten/KostenD/Kj_wd_oh;
			attribute<Eur_yr> Kj_wd_adm       (PlanRegio) := R_StadV * StartJaar/StateNaAllocatie/HerKostenBaten/KostenD/Kj_wd_adm;
			
			attribute<Eur_yr> Kj_pt_oh        (PlanRegio) := R_StadV * Startjaar/GebiedsAllocatie/RestWarmte/lastiter/Assignment/Kj_pt_oh;
			attribute<Eur_yr> Kj_pt_adm       (PlanRegio) := R_StadV * Startjaar/GebiedsAllocatie/RestWarmte/lastiter/Assignment/Kj_pt_adm;
			attribute<Eur_yr> Kj_ow_oh        (PlanRegio) := R_StadV * Startjaar/GebiedsAllocatie/RestWarmte/lastiter/Assignment/Kj_ow_oh;
			attribute<Eur_yr> Kj_ow_adm       (PlanRegio) := R_StadV * Startjaar/GebiedsAllocatie/RestWarmte/lastiter/Assignment/Kj_ow_adm;
			attribute<Eur_yr> Kj_gnet_oh      (PlanRegio) := R_StadV * Netwerken/Aardgas/Kj_oh;
			attribute<Eur_yr> Kj_enet_oh      (PlanRegio) := R_StadV * Netwerken/Elektriciteit/Kj_oh;
			attribute<nrAansl>nrAansl_w_MT    (PlanRegio) := R_StadV * sum(StateNaAllocatie/bebouwing/BestaandeWoning/nrAansluitingen, StateNaAllocatie/bebouwing/BestaandeWoning/PlanRegio_rel);
			attribute<nrAansl>nrAansl_u_MT    (PlanRegio) := R_StadV * sum(StateNaAllocatie/bebouwing/BestaandeUtil/nrAansluitingen  , StateNaAllocatie/bebouwing/BestaandeUtil/PlanRegio_rel  );
			attribute<Eur_yr> Kj_elek_gebouw  (PlanRegio) := R_StadV * maatschappelijk/gebouwen/ElektriciteitsVraag/Km_Totaal;
		}
		container OrigKosten
		{
			container table
			{
				attribute<Eur_yr> K01_Enet_verzwaren         (PlanRegio) := const(0.0[eur_yr], PlanRegio);
				attribute<Eur_yr> K02_Gnet_verwijderen       (PlanRegio) := const(0.0[eur_yr], PlanRegio);
				attribute<Eur_yr> K03_Gnet_aanpassen         (PlanRegio) := const(0.0[eur_yr], PlanRegio);
				attribute<Eur_yr> K04_Wnet_buurt             (PlanRegio) := const(0.0[eur_yr], PlanRegio);
				attribute<Eur_yr> K05_Wnet_pand              (PlanRegio) := const(0.0[eur_yr], PlanRegio);
				attribute<Eur_yr> K06_Wnet_transport         (PlanRegio) := const(0.0[eur_yr], PlanRegio);
				attribute<Eur_yr> K07_Wnet_opwekking         (PlanRegio) := const(0.0[eur_yr], PlanRegio);
				attribute<Eur_yr> K08_Gebouw_schil           (PlanRegio) := const(0.0[eur_yr], PlanRegio);
				attribute<Eur_yr> K09_Gebouw_installatie     (PlanRegio) := const(0.0[eur_yr], PlanRegio);
				attribute<Eur_yr> K10_Totaal_Kapitaalslasten (PlanRegio) := const(0.0[eur_yr], PlanRegio);
				attribute<Eur_yr> K11_Inkoop_Warmte          (PlanRegio) := const(0.0[eur_yr], PlanRegio);
				attribute<Eur_yr> K12_Inkoop_Gas             (PlanRegio) := const(0.0[eur_yr], PlanRegio);
				attribute<Eur_yr> K13_Inkoop_Elek            (PlanRegio) := const(0.0[eur_yr], PlanRegio);
				attribute<Eur_yr> K14_Gebouw_OenM            (PlanRegio) := const(0.0[eur_yr], PlanRegio);
				attribute<Eur_yr> K15_Wnet_OenM              (PlanRegio) := const(0.0[eur_yr], PlanRegio);
				attribute<Eur_yr> K16_Enet_Gnet_OenM         (PlanRegio) := const(0.0[eur_yr], PlanRegio);
			}
			container TotaalUitstoot
			{
				attribute<Eur_yr> Natkosten_abs (Planregio) := const(0.0[eur_yr], PlanRegio);
			}
			attribute<ton_yr> Orig_CO2_ton (Planregio) :=
				  StartJaar/EnergyResults/CO2/gebouwen/Aardgas/totaal / 1000.0[kg_yr / ton_yr]
				+ StartJaar/EnergyResults/CO2/GebiedsOpties/totaal    / 1000.0[kg_yr / ton_yr];
		}
	}
	container Ref2030        := CalculationSchemes/LeidraadResultaten(Classifications/RekenStap/Name[uint8(1)])
	{	
		container WarmteBestaand //TODO: overweeg overerving van StartJaar/WarmteBestaand, er lijkt veel overlap
		{
			attribute<Ratio>  R_StadV         (PlanRegio) := Invoer/RuimtelijkeData/StudieGebied/buurt/R_StadV;
			
			attribute<GJ_yr>  V_Bijstook      (PlanRegio) := R_StadV * StateNaAllocatie/KostenBaten/KostenD/Bijstook/V_Bijstook;
			attribute<GJ_yr>  V_wd_elek       (PlanRegio) := R_StadV * StateNaAllocatie/KostenBaten/KostenD/SecundairNet/V_wd_elek;
			attribute<GJ_yr>  V_ow_warmte     (PlanRegio) := R_StadV * StateNaAllocatie/KostenBaten/KostenD/V_warmte;
			attribute<Eur_yr> Kj_ow_productie (PlanRegio) := R_StadV * GebiedsAllocatie/RestWarmte/Iterations/I0/Assignment/Km_WarmteFactor * StateNaAllocatie/KostenBaten/V_WarmtePrimairD;
			attribute<Eur_yr> Kj_wd_gas       (PlanRegio) := R_StadV * (1d - GebiedsAllocatie/RestWarmte/results/RestWarmte/P_vol) * StateNaAllocatie/KostenBaten/KostenD/Bijstook/Km_wd_gas;
			attribute<Eur_yr> Kj_wd_elek      (PlanRegio) := R_StadV * StateNaAllocatie/KostenBaten/KostenD/SecundairNet/Km_wd_elek;
			attribute<Eur_yr> Kj_lv_admin     (PlanRegio) := R_StadV * StateNaAllocatie/KostenBaten/Opbrengsten/JaarlijksD/Oj_vastrecht * Tussenresultaten/Leercurves/OenM/Curve * Kengetallen/Onderhoud/R_lv_Admin * 1[yr];
			
			//TODO FvdM: in de volgende 3 attributen echtHerKostenBaten nodig? Zoniet,dan verwijderingHerKostenBatenoverwegen.
			
			attribute<Eur_yr> Kj_id_oh        (PlanRegio) := R_StadV * StartJaar/StateNaAllocatie/HerKostenBaten/KostenD/Kj_id_oh;
			attribute<Eur_yr> Kj_wd_oh        (PlanRegio) := R_StadV * StartJaar/StateNaAllocatie/HerKostenBaten/KostenD/Kj_wd_oh;
			attribute<Eur_yr> Kj_wd_adm       (PlanRegio) := R_StadV * StartJaar/StateNaAllocatie/HerKostenBaten/KostenD/Kj_wd_adm;
			
			attribute<Eur_yr> Kj_pt_oh        (PlanRegio) := R_StadV * Startjaar/GebiedsAllocatie/RestWarmte/lastiter/Assignment/Kj_pt_oh;
			attribute<Eur_yr> Kj_pt_adm       (PlanRegio) := R_StadV * Startjaar/GebiedsAllocatie/RestWarmte/lastiter/Assignment/Kj_pt_adm;
			attribute<Eur_yr> Kj_ow_oh        (PlanRegio) := R_StadV * Startjaar/GebiedsAllocatie/RestWarmte/lastiter/Assignment/Kj_ow_oh;
			attribute<Eur_yr> Kj_ow_adm       (PlanRegio) := R_StadV * Startjaar/GebiedsAllocatie/RestWarmte/lastiter/Assignment/Kj_ow_adm;
			attribute<Eur_yr> Kj_gnet_oh      (PlanRegio) := R_StadV * Netwerken/Aardgas/Kj_oh;
			attribute<Eur_yr> Kj_enet_oh      (PlanRegio) := R_StadV * Netwerken/Elektriciteit/Kj_oh;
			
			attribute<nrAansl>nrAansl_w_MT    (PlanRegio) := R_StadV * sum(StateNaAllocatie/bebouwing/BestaandeWoning/nrAansluitingen, StateNaAllocatie/bebouwing/BestaandeWoning/PlanRegio_rel);
			attribute<nrAansl>nrAansl_u_MT    (PlanRegio) := R_StadV * sum(StateNaAllocatie/bebouwing/BestaandeUtil/nrAansluitingen  , StateNaAllocatie/bebouwing/BestaandeUtil/PlanRegio_rel  );
			attribute<Eur_yr> Kj_elek_gebouw  (PlanRegio) := R_StadV * maatschappelijk/gebouwen/ElektriciteitsVraag/Km_Totaal;
		}
		//container OrigKosten := Startjaar/PlanregioResults/NationaleKosten;
		container OrigKosten := Startjaar/OrigKosten;		
	}
	container Zichtjaar := CalculationSchemes/LeidraadResultaten(last(Classifications/RekenStap/Name))
	{
		container OrigKosten     := Ref2030/PlanregioResults/NationaleKosten
		{
			container Table := Ref2030/PlanregioResults/NationaleKosten/Table
			{
				attribute<eur_yr> K09_Gebouw_installatie     (PlanRegio) := Ref2030/PlanregioResults/NationaleKosten/Recap/K09_Gebouw_installatie;
				attribute<eur_yr> K10_Totaal_Kapitaalslasten (PlanRegio) := Ref2030/PlanregioResults/NationaleKosten/Recap/K09_Gebouw_installatie;
			}
			attribute<ton_yr> Orig_CO2_ton (Planregio) := Ref2030/PlanregioResults/NationaleKosten/TotaalUitstoot/New_CO2_ton;
		}
		//TEST
		container Warmtebestaand := Ref2030/WarmteBestaand
		{

			attribute<Ratio>  R_StadV         (PlanRegio) := Invoer/RuimtelijkeData/StudieGebied/buurt/R_StadV;
			
			attribute<GJ_yr>  V_Bijstook      (PlanRegio) := R_StadV * ..../tussenresultaten/R2_2030/GebiedsAllocatie/RestWarmte/KostenBaten/KostenD/Bijstook/V_Bijstook;
			attribute<GJ_yr>  V_wd_elek       (PlanRegio) := R_StadV * ..../tussenresultaten/R2_2030/GebiedsAllocatie/RestWarmte/KostenBaten/KostenD/SecundairNet/V_wd_elek;
			attribute<GJ_yr>  V_ow_warmte     (PlanRegio) := R_StadV * ..../tussenresultaten/R2_2030/GebiedsAllocatie/RestWarmte/KostenBaten/KostenD/V_warmte;
			attribute<Eur_yr> Kj_ow_productie (PlanRegio) := R_StadV * GebiedsAllocatie/RestWarmte/Iterations/I0/Assignment/Km_WarmteFactor * ..../tussenresultaten/R2_2030/GebiedsAllocatie/RestWarmte/KostenBaten/V_WarmtePrimairD;
			attribute<Eur_yr> Kj_wd_gas       (PlanRegio) := R_StadV * (1d - GebiedsAllocatie/RestWarmte/results/RestWarmte/P_vol) * ..../tussenresultaten/R2_2030/GebiedsAllocatie/RestWarmte/KostenBaten/KostenD/Bijstook/Km_wd_gas;
			attribute<Eur_yr> Kj_wd_elek      (PlanRegio) := R_StadV * ..../tussenresultaten/R2_2030/GebiedsAllocatie/RestWarmte/KostenBaten/KostenD/SecundairNet/Km_wd_elek;
		}
		//TEST
	}
}