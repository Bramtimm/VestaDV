container Vergelijking
{		
	container meerkosten := Invoer/RuimtelijkeData/Studiegebied/buurt, DialogType = "map", DialogData = "geometry"
	{
		attribute<Eur_ton>	s1a (Planregio):= results_s1a/results/H13_Nat_meerkost_CO2;
		attribute<Eur_ton>	s1b (Planregio):= results_s1b/results/H13_Nat_meerkost_CO2;
		
		attribute<Eur_ton>	s2a (Planregio):= results_s2a/results/H13_Nat_meerkost_CO2;
		attribute<Eur_ton>	s2b (Planregio):= results_s2b/results/H13_Nat_meerkost_CO2;
		attribute<Eur_ton>	s2c (Planregio):= results_s2c/results/H13_Nat_meerkost_CO2;
		attribute<Eur_ton>	s2d (Planregio):= results_s2d/results/H13_Nat_meerkost_CO2;
		
		attribute<Eur_ton>	s3a (Planregio):= results_s3a/results/H13_Nat_meerkost_CO2;
		attribute<Eur_ton>	s3b (Planregio):= results_s3b/results/H13_Nat_meerkost_CO2;
		attribute<Eur_ton>	s3c (Planregio):= results_s3c/results/H13_Nat_meerkost_CO2;
		attribute<Eur_ton>	s3d (Planregio):= results_s3d/results/H13_Nat_meerkost_CO2;
		attribute<Eur_ton>	s3e (Planregio):= results_s3e/results/H13_Nat_meerkost_CO2;
		
		attribute<Eur_ton>	s4a (Planregio):= results_s4a/results/H13_Nat_meerkost_CO2;
		attribute<Eur_ton>	s5a (Planregio):= results_s5a/results/H13_Nat_meerkost_CO2;
		
		
		attribute<Variant>	Welke_S1 (PlanRegio) := Statisch/Uitgesloten_bool ? Variant/V/geen : (s1a > s1b ? Variant/V/s1b : Variant/V/s1a);
		attribute<string>	S1_naam  (PlanRegio) := Variant/name[Welke_S1];
		attribute<Eur_ton>	s1       (Planregio) := min_elem(s1a,s1b);
		attribute<Variant>	Welke_S2 (PlanRegio) := Statisch/Uitgesloten_bool ? Variant/V/geen : temp/s2a_of_s2b_naam;
		attribute<Eur_ton>	s2       (Planregio) := min_elem(s2a,s2b);
		attribute<string>	S2_naam  (PlanRegio) := Variant/name[Welke_S2];
		attribute<Variant>	Welke_S3 (PlanRegio) := Statisch/Uitgesloten_bool ? Variant/V/geen : ((s3e == s3a) && (s3e == s3b) && (s3e == s3c) && (s3e == s3d)) ? Welke_S1 : (s3e > temp/s3abcd_waarde ? temp/s3abcd_naam : Variant/V/s3e);
		attribute<Eur_ton>	s3       (Planregio) := min_elem(s3a,s3b,s3c,s3d,s3e);
		attribute<string>	S3_naam  (PlanRegio) := Variant/name[Welke_S3];
		attribute<Variant>	Welke_S4 (PlanRegio) := Statisch/Uitgesloten_bool ? Variant/V/geen : Variant/V/s4a;
		attribute<Eur_ton>	s4       (PlanRegio) := s4a;
		attribute<string>	S4_naam  (PlanRegio) := Variant/name[Welke_S4];
		attribute<Variant>	Welke_S5 (PlanRegio) := Statisch/Uitgesloten_bool ? Variant/V/geen : Variant/V/s5a;
		attribute<Eur_ton>	s5       (PlanRegio) := s5a;
		attribute<string>	S5_naam  (PlanRegio) := Variant/name[Welke_S5];
		
		attribute<Variant>	Welke_S6 (PlanRegio) := Statisch/Uitgesloten_bool ? Variant/V/geen : Beste_variant;
		attribute<Eur_ton>	s6       (PlanRegio) := Beste_eur_ton;
		attribute<string>	S6_naam  (PlanRegio) := Variant/name[Welke_S6];
		
		attribute<Variant>	Welke_S6_GGcorr (PlanRegio) := Groengaswaarde/Variant_S6;
		attribute<string>	S6_naam_GGcorr  (PlanRegio) := Variant/name[Welke_S6_GGcorr];
		
		attribute<Variant>	S1_of_S2_of_S3_naam   (PlanRegio) := Statisch/Uitgesloten_bool ? Variant/V/geen : (s1 > s2 ? s2 : s1) > s3 ? welke_s3 : (s1 > s2 ? welke_s2 : welke_s1);
		attribute<Eur_ton>	S1_of_S2_of_S3_waarde (PlanRegio) :=(s1 > s2 ? s2 : s1) > s3 ? s3 : (s1 > s2 ? s2 : s1);
		attribute<string>	S1_of_S2_of_S3_str    (PlanRegio) := Variant/name[S1_of_S2_of_S3_naam];
		attribute<Variant>	S4_of_S5_naam         (PlanRegio) := Statisch/Uitgesloten_bool ? Variant/V/geen : (s4 > s5 ? welke_s5 : welke_s4);
		attribute<Eur_ton>	S4_of_S5_waarde       (PlanRegio) := (s4 > s5 ? s5 : s4);
		attribute<string>	S4_of_S5_str          (PlanRegio) := Variant/name[S4_of_S5_naam];
		
		
		attribute<Variant>	Beste_variant         (PlanRegio) := (S4_of_S5_waarde > S1_of_S2_of_S3_waarde ? S1_of_S2_of_S3_naam : S4_of_S5_naam);
		attribute<string>	Beste_variant_str     (PlanRegio) := Variant/name[Beste_variant];
		attribute<Eur_ton>	Beste_eur_ton         (PlanRegio) := (S4_of_S5_waarde > S1_of_S2_of_S3_waarde ? S1_of_S2_of_S3_waarde : S4_of_S5_waarde);
		
		attribute<Variant>	Beste_variant_beschikbaar         (PlanRegio) := GroengasWaarde/Variant_S6;
		attribute<string>	Beste_variant_beschikbaar_str     (PlanRegio) := Variant/name[Beste_variant_beschikbaar];
		
		container GroenGasWaarde
		{
			attribute<string>		BU_CODE               (PlanRegio) :=	Statisch/Table/BU_CODE;
			attribute<m3_yr>		Aantal_m3_S4          (PlanRegio) :=	(Scenario_4/HoofdIndicatoren/H03_Vraag_groengas * nrWEQ) / Invoer/Kengetallen/Constanten/GasEnergie;
			attribute<m3_yr>		Aantal_m3_S5          (PlanRegio) :=	(Scenario_5/HoofdIndicatoren/H03_Vraag_groengas * nrWEQ) / Invoer/Kengetallen/Constanten/GasEnergie;
			attribute<m3_yr>		Aantal_m3_S6          (PlanRegio) :=	(Scenario_6_tussenstap/HoofdIndicatoren/H03_Vraag_groengas * nrWEQ) / Invoer/Kengetallen/Constanten/GasEnergie;
			
			attribute<eur_yr>		Meerkosten_S123       (PlanRegio) :=	S1_of_S2_of_S3_str == 's1a' ? Results_s1a/results/H12_Nat_meerkosten : 
																			S1_of_S2_of_S3_str == 's1b' ? Results_s1b/results/H12_Nat_meerkosten : 
																			S1_of_S2_of_S3_str == 's2a' ? Results_s2a/results/H12_Nat_meerkosten : 
																			S1_of_S2_of_S3_str == 's2b' ? Results_s2b/results/H12_Nat_meerkosten : 
																			S1_of_S2_of_S3_str == 's2c' ? Results_s2c/results/H12_Nat_meerkosten : 
																			S1_of_S2_of_S3_str == 's2d' ? Results_s2d/results/H12_Nat_meerkosten : 
																			S1_of_S2_of_S3_str == 's3a' ? Results_s3a/results/H12_Nat_meerkosten : 
																			S1_of_S2_of_S3_str == 's3b' ? Results_s3b/results/H12_Nat_meerkosten : 
																			S1_of_S2_of_S3_str == 's3c' ? Results_s3c/results/H12_Nat_meerkosten : 
																			S1_of_S2_of_S3_str == 's3d' ? Results_s3d/results/H12_Nat_meerkosten : 
																			S1_of_S2_of_S3_str == 's3e' ? Results_s3e/results/H12_Nat_meerkosten : 0.0[eur_yr]; 
			
			attribute<eur_yr>		Meerkosten_S4         (PlanRegio) :=	Scenario_4/Hoofdindicatoren/H12_Nat_meerkosten;
			attribute<eur_yr>		Meerkosten_S5         (PlanRegio) :=	Scenario_5/Hoofdindicatoren/H12_Nat_meerkosten;
			attribute<eur_yr>		Meerkosten_S6         (PlanRegio) :=	S4_of_S5_str == 's4a' ? Results_s4a/results/H12_Nat_meerkosten : 
																			S4_of_S5_str == 's5a' ? Results_s5a/results/H12_Nat_meerkosten : 0.0[eur_yr];
			
			attribute<eur_yr>		Besparing_S4          (PlanRegio) :=	Meerkosten_S123 - Meerkosten_S4;
			attribute<eur_yr>		Besparing_S5          (PlanRegio) :=	Meerkosten_S123 - Meerkosten_S5;
			attribute<eur_yr>		Besparing_S6          (PlanRegio) :=	Meerkosten_S123 - Meerkosten_S6;
			
			attribute<Eur_m3>		GroenGasWaarde_S4     (PlanRegio) :=	0d - (Besparing_S4 / Aantal_m3_S4);
			attribute<Eur_m3>		GroenGasWaarde_S5     (PlanRegio) :=	0d - (Besparing_S5 / Aantal_m3_S5);
			attribute<Eur_m3>		GroenGasWaarde_S6     (PlanRegio) :=	0d - (Besparing_S6 / Aantal_m3_S6);
			
			parameter<m3_yr>		Totaal_GG_beschikbaar := 1500000000[m3_yr];
			
			parameter<Eur_m3>		threshold_s4  := nth_element_weighted(GroenGasWaarde_S4, Totaal_GG_beschikbaar, Aantal_m3_S4);
			parameter<Eur_m3>		threshold_s5  := nth_element_weighted(GroenGasWaarde_S5, Totaal_GG_beschikbaar, Aantal_m3_S5);
			parameter<Eur_m3>		threshold_s6  := nth_element_weighted(GroenGasWaarde_S6, Totaal_GG_beschikbaar, Aantal_m3_S6);
			
			attribute<Bool>			Beschikbaarheid_S4    (PlanRegio) :=	GroenGasWaarde_S4 <= threshold_s4;
			attribute<Bool>			Beschikbaarheid_S5    (PlanRegio) :=	GroenGasWaarde_S5 <= threshold_s5;	
			attribute<Bool>			Beschikbaarheid_S6    (PlanRegio) :=	GroenGasWaarde_S6 <= threshold_s6;	
			
			attribute<variant>		Variant_S6 (PlanRegio) := Beschikbaarheid_S6 == true? S4_of_S5_naam : S1_of_S2_of_S3_naam;
		}	
		
		container Scenario_1 
		{
			Container Hoofdindicatoren := MaakHoofdIndicatoren(S1_naam);
			Container NationaleKosten := MaakNationaleKosten(S1_naam);
			Container Aansluitingen := MaakAansluitingen(S1_naam);
		}
		container Scenario_2 
		{
			Container Hoofdindicatoren := MaakHoofdIndicatoren(S2_naam);
			Container NationaleKosten := MaakNationaleKosten(S2_naam);
			Container Aansluitingen := MaakAansluitingen(S2_naam);
		}
		container Scenario_3 
		{
			Container Hoofdindicatoren := MaakHoofdIndicatoren(S3_naam);
			Container NationaleKosten := MaakNationaleKosten(S3_naam);
			Container Aansluitingen := MaakAansluitingen(S3_naam);
		}
		container Scenario_4 
		{
			Container Hoofdindicatoren := MaakHoofdIndicatoren(S4_naam);
			Container NationaleKosten := MaakNationaleKosten(S4_naam);
			Container Aansluitingen := MaakAansluitingen(S4_naam);
		}
		container Scenario_5 
		{
			Container Hoofdindicatoren := MaakHoofdIndicatoren(S5_naam);
			Container NationaleKosten := MaakNationaleKosten(S5_naam);
			Container Aansluitingen := MaakAansluitingen(S5_naam);
		}
		container Scenario_6_tussenstap 
		{
			Container Hoofdindicatoren := MaakHoofdIndicatoren(S6_naam);
			Container NationaleKosten := MaakNationaleKosten(S6_naam);
			Container Aansluitingen := MaakAansluitingen(S6_naam);
		}
		container Scenario_6 
		{
			Container Hoofdindicatoren := MaakHoofdIndicatoren(S6_naam_GGcorr);
			Container NationaleKosten := MaakNationaleKosten(S6_naam_GGcorr);
			Container Aansluitingen := MaakAansluitingen(S6_naam_GGcorr);
		}
		
		template MaakHoofdIndicatoren
		{
			//parameters
			attribute<string> Scode (Planregio);
			//parameters
			
			attribute<string>		V01_Strategievariant	(Planregio):=	Scode;
			attribute<GJ_yr_WEQ>	H01_warmtetotaal		(Planregio):=	Scode == 's1a' ? Results_s1a/results/H01_warmtetotaal : 
																			Scode == 's1b' ? Results_s1b/results/H01_warmtetotaal : 
																			Scode == 's2a' ? Results_s2a/results/H01_warmtetotaal : 
																			Scode == 's2b' ? Results_s2b/results/H01_warmtetotaal : 
																			Scode == 's2c' ? Results_s2c/results/H01_warmtetotaal : 
																			Scode == 's2d' ? Results_s2d/results/H01_warmtetotaal : 
																			Scode == 's3a' ? Results_s3a/results/H01_warmtetotaal : 
																			Scode == 's3b' ? Results_s3b/results/H01_warmtetotaal : 
																			Scode == 's3c' ? Results_s3c/results/H01_warmtetotaal : 
																			Scode == 's3d' ? Results_s3d/results/H01_warmtetotaal : 
																			Scode == 's3e' ? Results_s3e/results/H01_warmtetotaal : 
																			Scode == 's4a' ? Results_s4a/results/H01_warmtetotaal : 
																			Scode == 's5a' ? Results_s5a/results/H01_warmtetotaal : 0.0[GJ_yr_WEQ];
																			
			attribute<GJ_yr_WEQ>	H02_Vraag_aardgas		(Planregio):=	Scode == 's1a' ? Results_s1a/results/H02_vraag_aardgas : 
																			Scode == 's1b' ? Results_s1b/results/H02_vraag_aardgas : 
																			Scode == 's2a' ? Results_s2a/results/H02_vraag_aardgas : 
																			Scode == 's2b' ? Results_s2b/results/H02_vraag_aardgas : 
																			Scode == 's2c' ? Results_s2c/results/H02_vraag_aardgas : 
																			Scode == 's2d' ? Results_s2d/results/H02_vraag_aardgas : 
																			Scode == 's3a' ? Results_s3a/results/H02_vraag_aardgas : 
																			Scode == 's3b' ? Results_s3b/results/H02_vraag_aardgas : 
																			Scode == 's3c' ? Results_s3c/results/H02_vraag_aardgas : 
																			Scode == 's3d' ? Results_s3d/results/H02_vraag_aardgas : 
